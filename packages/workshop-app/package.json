{
  "name": "@kentcdodds/workshop-app",
  "sideEffects": false,
  "bin": {
    "kcdshop": "./bin/kcdshop.js"
  },
  "files": [
    "dist",
    "index.js",
    "components.js",
    "components-build",
    "server-build",
    "build",
    "public"
  ],
  "scripts": {
    "build": "run-s --print-label --print-name build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "build:server": "tsx ./other/build-dir.ts server",
    "build:comps": "tsx ./other/build-dir.ts components",
    "build:decs": "tsc --build ./components/tsconfig.json",
    "dev": "cross-env NODE_ENV=development FORCE_COLOR=true run-p --print-label --print-name dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "remix watch",
    "dev:server": "tsx watch --clear-screen=false --ignore \"app/**\" --ignore \"build/**\" --ignore \"node_modules/**\" ./index.js",
    "dev:comps": "tsx --watch --no-warnings --watch-preserve-output --watch-path ./components ./other/build-dir.ts components",
    "dev:decs": "npm run build:decs --silent -- --preserveWatchOutput --watch",
    "format": "prettier --write .",
    "generate:css": "tailwindcss -o ./app/styles/tailwind.css",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "cross-env NODE_ENV=production node .",
    "typecheck": "tsc -b",
    "validate": "run-p \"test -- --run\" lint typecheck"
  },
  "dependencies": {
    "@kentcdodds/md-temp": "^3.1.1",
    "@reach/tabs": "^0.18.0",
    "@remix-run/express": "^1.7.5",
    "@remix-run/node": "^1.7.5",
    "@remix-run/react": "^1.7.5",
    "@remix-run/server-runtime": "^1.7.5",
    "address": "^1.2.2",
    "cachified": "^3.1.0",
    "chalk": "^5.2.0",
    "close-with-grace": "^1.1.0",
    "clsx": "^1.2.1",
    "common-ancestor-path": "^1.0.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "fkill": "^8.1.0",
    "fs-extra": "^11.1.0",
    "glob": "^8.1.0",
    "globby": "^13.1.3",
    "isbot": "^3.6.3",
    "lru-cache": "^7.14.1",
    "match-sorter": "^6.3.1",
    "md5-hex": "^4.0.0",
    "mdast-util-from-markdown": "^1.2.0",
    "mdast-util-mdx": "^2.0.1",
    "mdast-util-to-markdown": "^1.5.0",
    "mdx-bundler": "^9.2.1",
    "micromark-extension-mdxjs": "^1.0.0",
    "morgan": "^1.10.0",
    "parse-git-diff": "^0.0.7",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rehype-stringify": "^9.0.3",
    "remark-autolink-headings": "^7.0.1",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "remix-flat-routes": "^0.5.7",
    "shell-quote": "^1.8.0",
    "shiki": "^0.14.0",
    "tiny-invariant": "^1.3.1",
    "tsx": "^3.12.2",
    "unified": "^10.1.2",
    "unist-util-visit": "^4.1.2",
    "wait-on": "^7.0.1",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.7.5",
    "@remix-run/eslint-config": "^1.7.5",
    "@tailwindcss/typography": "^0.5.9",
    "@types/common-ancestor-path": "^1.0.0",
    "@types/compression": "^1.7.2",
    "@types/eslint": "^8.4.10",
    "@types/express": "^4.17.16",
    "@types/fs-extra": "^11.0.1",
    "@types/lru-cache": "^7.10.10",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.11.9",
    "@types/prettier": "^2.7.2",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@types/shell-quote": "^1.7.1",
    "@types/wait-on": "^5.3.1",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "execa": "^6.1.0",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.2.2",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": "18"
  }
}
